<CustomOneLineIconListItemh>
    size_hint: .98,.73
    # pos_hint: {"center_x": .5, "center_y": .5}
    ripple_behavior: False
    adaptive_size: True
    padding: "10dp"
    border: dp(5)
    elevation: 0
    # radius: 10
    # on_release: root.show_example_list_bottom_sheet()
    MDFloatLayout:
        # md_bg_color: 1,1,1,1
        FitImage:
            id: img
            source: root.image
            size_hint: None, None
            size: dp(40), dp(40)
            radius: [self.width/2,]
            pos_hint:{"center_x":.05,"center_y":.52}
        MDFloatLayout:
            # size_hint: 0.98,.16
            # md_bg_color:rgba(0,0,0,255)
            pos_hint:{"center_x":0.5,"center_y":.95}
            
            MDLabel:        
                text:"Paid"
                # halign: "left"
                font_name: 'assets/fonts/EncodeSans-Regular.ttf'
                font_size: "13sp"
                # halign:"center"
                pos_hint:{"center_x":1,"center_y":.52}
                color:  "#7E8899"
            MDLabel:        
                text:"Due"
                # halign: "left"
                font_name: 'assets/fonts/EncodeSans-Regular.ttf'
                font_size: "13sp"
                # halign:"center"
                pos_hint:{"center_x":1.15,"center_y":.52}
                color:  "#7E8899"
            
           

            MDLabel:        
                text:"Loan"
                # halign: "left"
                font_name: 'assets/fonts/EncodeSans-Regular.ttf'
                font_size: "13sp"
                # halign:"center"
                pos_hint:{"center_x":1.3,"center_y":.52}
                color: "#FF5252" if self.text=="Debit" else "#08A045"
        
        MDLabel:        
            text: root.Name[0:12]+".."
            markup:True
            font_name: 'assets/fonts/EncodeSans-SemiBold.ttf'
            font_size: "13sp"
            pos_hint:{"center_x":.65,"center_y":.40}
        MDLabel:        
            text:str(root.Paid)
            markup:True
            font_name: 'assets/fonts/Roboto-Regular.ttf'
            font_size: "15sp"
            pos_hint:{"center_x":1,"center_y":.20}
        MDLabel:        
            text:str(root.Due)
            markup:True
            font_name: 'assets/fonts/Roboto-Regular.ttf'
            font_size: "15sp"
            pos_hint:{"center_x":1.15,"center_y":.20}
        # TooltipMDIconButton:
        #     icon: "information"
        #     tooltip_text: root.Name
        #     pos_hint: {"center_x":.45,"center_y":.40}
        MDLabel:        
            text:str(root.Loan)
            markup:True
            font_name: 'assets/fonts/Roboto-Regular.ttf'
            font_size: "15sp"
            pos_hint:{"center_x":1.3,"center_y":.20}
        MDIconButton:
            icon: "chevron-right"
            pos_hint:{"center_x":.98,"center_y":.52}
            on_release:
                app.screen_manager.get_screen("holdings").KJ(root.Name)
<TooltipMDIconButton@MDIconButton+MDTooltip>
<Holdings>:
    name: "holdings"
    
    MDBoxLayout:
        orientation: "vertical"
        md_bg_color: "#F5F5F5"
        MDTopAppBar:
            title: "Tessa"
            elevation: 2
            pos_hint: {"top": 1}
            md_bg_color: "#F8B22D"
            specific_text_color: "#4a4939"
            left_action_items: [["menu", lambda x: (nav_drawer.set_state("open") if root.usertype == "Admin" else nav_drawer_member.set_state("open"))]]
        MDBoxLayout:
            orientation: 'vertical'
            spacing: dp(10)
            padding: dp(20)
            MDBoxLayout:
                adaptive_size: True
                # size_hint: 1,.18
                spacing: "12dp"
                

                MDFlatButton:
                    id: grp
                    text: "Group Folio"                    
                    md_bg_color: "#F8B22D"
                    # on_release: print("T1")


                MDFlatButton:
                    id: ind
                    text: "Your Folio"
                    md_bg_color: "#FFFFFF"                    
                    on_release: app.screen_manager.get_screen("holdings").KJ("Abhijit Shinde",False)
                MDIconButton:
                    icon: "progress-download"
                    pos_hint: {"center_x": 0.5, "center_y": .5}
                    on_release: print("Download started...")
        
